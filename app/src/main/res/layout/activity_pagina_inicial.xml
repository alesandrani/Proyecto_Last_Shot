<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#FFFFFF"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/rxuo59pr2e5h"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#D2F7EC"
        android:paddingTop="33dp"
        android:clipChildren="false"
        android:clipToPadding="false">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/rumfs3dbdl2"
                android:layout_width="104dp"
                android:layout_height="104dp"
                android:layout_marginBottom="50dp"
                android:layout_gravity="center_horizontal"
                android:src="@mipmap/ic_launcher"
                android:scaleType="fitCenter"
                android:adjustViewBounds="true" />

            <ImageView
                android:id="@+id/rrfkyk5wjuh9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="50dp"
                android:layout_gravity="center_horizontal"
                android:src="@mipmap/ic_launcher"
                android:scaleType="fitCenter"
                android:adjustViewBounds="true" />

            <RelativeLayout
                android:id="@+id/rt2yypxez2yi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clipChildren="false"
                android:clipToPadding="false">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/rb64yvt3j4wg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/s000000sw2cr30bff69b4"
                        android:padding="16dp"
                        android:elevation="4dp"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/rbaxguoevnfp"
                            android:layout_width="119dp"
                            android:layout_height="84dp"
                            android:layout_marginTop="38dp"
                            android:layout_gravity="center_horizontal"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitCenter"
                            android:adjustViewBounds="true" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/r7s7bbrzze53"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#FFE135"
                    android:textSize="48sp"
                    android:layout_centerHorizontal="true"
                    android:text="A BEBER"
                    android:padding="16dp"
                    android:textStyle="bold" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
